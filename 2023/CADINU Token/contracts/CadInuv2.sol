        return balanceOf(address(this));
    }

    function getBNBBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function withdrawBnb(uint256 _amount) public onlyOwner {
        payable(msg.sender).transfer(_amount);
    }

    function setMigrationSetting(address _migration, uint256 _newTokenConvertMul, uint256 _newTokenConvertDiv) public onlyOwner {
        migration = _migration;
        newTokenConvertMul = _newTokenConvertMul;
        newTokenConvertDiv = _newTokenConvertDiv;
    }

    function isMigrationStarted() external view returns (bool) {
        return migration != address(0);
    }

    function migrate(uint256 amount)
        external preventBlacklisted(_msgSender(), "Cadinu: Migrated account is blacklisted"){
        require(migration != address(0), "Cadinu: Migration is not started");
        ERC20Upgradeable(migration).transferFrom(_msgSender(), owner(), amount);
        uint256 newAmount;
        newAmount = (amount * newTokenConvertMul) / newTokenConvertDiv;
        _tokenTransfer(owner(), _msgSender(), newAmount, 0, false);
    }
}