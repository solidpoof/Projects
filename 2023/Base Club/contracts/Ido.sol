// File: contracts\Ido.sol

pragma solidity ^0.8.0;
contract Ido is Initializable, OwnableUpgradeable {

    using SafeMath for uint256;

    IERC20 public token;
    IUniswapV2Router02 public uniswapV2Router;
    address public burnAddress;

    uint256 public whitelistTime;
    uint256 public whitelistEndTime;
    uint256 public fairlaunchTime;
    uint256 public fairlaunchEndTime;
    uint256 public claimTime;
    bool public started;
    bool public refund;

    struct IdoInfo {
        uint256 softCap;
        uint256 hardCap;
        uint256 minCommit;
        uint256 maxCommit;
        uint256 totalCommit;
        uint256 rateToken;
        uint256 rateEth;
        bool claimed;
    }


    mapping(uint => IdoInfo) public idoInfo;
    mapping(address => uint) public whitelist;
    mapping(address => uint) public whitelistCommitted;
    mapping(address => uint) public whitelistClaimed;
    mapping(address => uint) public fairlaunchCommitted;
    mapping(address => uint) public fairlaunchClaimed;

    /*  ╔══════════════════════════════╗
        ║            EVENTS            ║
        ╚══════════════════════════════╝ */
    event CommitWhitelist(address user, uint256 amount);
    event ClaimWhitelistToken(address user, uint256 amount);
    event CommitFairlaunch(address user, uint256 amount);
    event ClaimFairlaunchToken(address user, uint256 amount);
    event RefundWhitelist(address user, uint256 amount);
    event RefundFairlaunch(address user, uint256 amount);


    function initialize(address _token, uint _whitelistTime, uint _whitelistEndTime, uint _fairlaunchTime, uint _fairlaunchEndTime, uint _claimTime) public initializer {
        __Ownable_init();
        token = IERC20(_token);
        whitelistTime = _whitelistTime;
        whitelistEndTime = _whitelistEndTime;
        fairlaunchTime = _fairlaunchTime;
        fairlaunchEndTime = _fairlaunchEndTime;
        claimTime = _claimTime;
        started = false;
        refund = false;
        burnAddress = address(0x000000000000000000000000000000000000dEaD);

          //Mainnet
          idoInfo[0].softCap = 0 ether;
          idoInfo[0].hardCap = 20 ether;
          idoInfo[0].minCommit = 0.3 ether;
          idoInfo[0].maxCommit = 20 ether;
          idoInfo[0].rateToken = 40_000_000 ether;
          idoInfo[0].rateEth = 20 ether;

          idoInfo[1].softCap = 0 ether;
          idoInfo[1].hardCap = 100 ether;
          idoInfo[1].minCommit = 0.1 ether;
          idoInfo[1].maxCommit = 1000 ether;
          idoInfo[1].rateToken = 200_000_000  ether;
          idoInfo[1].rateEth = 100 ether;

       uniswapV2Router = IUniswapV2Router02(0x327Df1E6de05895d2ab08513aaDD9313Fe505d86);
    }

    /*  ╔══════════════════════════════╗
        ║        ADMIN FUNCTIONS       ║
        ╚══════════════════════════════╝ */

    function setWhitelistSameAmount(address[] memory listAddress, uint amount) external onlyOwner {
        require(amount > 0, "amount > 0 !");
        for (uint i = 0; i < listAddress.length; i++) {
            whitelist[listAddress[i]] = amount;
        }
    }

    function start() external onlyOwner {
        require(!started, "started");
        token.transferFrom(msg.sender, address(this),idoInfo[0].rateToken + idoInfo[1].rateToken * 2);
        started=true;
    }

    function finalize() public onlyOwner {
        require(!refund, "refunded");
        require(!idoInfo[1].claimed, "finalized!");
        require(block.timestamp > fairlaunchEndTime, "Time invalid!");
        uint amountADesired = idoInfo[1].totalCommit >= idoInfo[1].rateEth ? idoInfo[1].rateToken : idoInfo[1].totalCommit.mul(idoInfo[1].rateToken).div(idoInfo[1].rateEth) ;
        uint amountAddLiquid = addLiquidityToDEX(amountADesired, idoInfo[1].totalCommit);
        if(amountAddLiquid < idoInfo[1].rateToken) token.transfer(burnAddress, idoInfo[1].rateToken.sub(amountAddLiquid)); //Refund remain token after add liquid
        (bool sent, ) = payable(msg.sender).call{value : address(this).balance}(""); //Total commit of whitelist and fairlaunch
        require(sent, "Failed to send Ether finalize");
        idoInfo[0].claimed = true;
        idoInfo[1].claimed = true;
    }

    function setRefundWhenLiquidFail() public onlyOwner {
        require(!idoInfo[1].claimed, "finalized!");
        require(block.timestamp > fairlaunchEndTime, "Time invalid!");
        token.transfer(owner(), token.balanceOf(address(this)));
        refund = true;
    }

    function ensureApprovalA() internal {
        uint entireBalance = token.balanceOf(address(this));

        // the token is being approved for the contract
        token.approve(address(uniswapV2Router), entireBalance);
    }

    function addLiquidityToDEX(uint256 amountADesired, uint ethAmount) internal returns (uint _amountA) {
        // approve
        ensureApprovalA();

        // add the liquidity
        (uint amountA, ,) =
        uniswapV2Router.addLiquidityETH{value : ethAmount}(
            address(token),
            amountADesired,
            0,
            0,
            owner(),
            (block.timestamp + 300)
        );
        return amountA;
    }

    /*  ╔══════════════════════════════╗
        ║        PUBLIC Function       ║
        ╚══════════════════════════════╝ */
    function commitWhitelist() payable external {
        //Check time
        uint _amount = msg.value;
        IdoInfo memory whitelistInfo = idoInfo[0];
        require(started, "not started");
        require(block.timestamp >= whitelistTime && block.timestamp <= whitelistEndTime, "Time invalid!");
        require(whitelistInfo.totalCommit < whitelistInfo.hardCap);
        require(whitelist[msg.sender] > 0, "not in whitelist !");
        require(whitelistInfo.maxCommit >= _amount + whitelistCommitted[msg.sender], "Insufficient value!");
        require(_amount + whitelistCommitted[msg.sender] >= whitelistInfo.minCommit, "value too small!");

        if (_amount + whitelistInfo.totalCommit <= whitelistInfo.hardCap) {
            whitelistCommitted[msg.sender] += _amount;
            idoInfo[0].totalCommit += _amount;

        } else {
            (bool sent, ) = payable(msg.sender).call{value : _amount.sub(whitelistInfo.hardCap.sub(whitelistInfo.totalCommit))}("");
            require(sent, "Failed to send Ether refund");
            whitelistCommitted[msg.sender] += whitelistInfo.hardCap.sub(whitelistInfo.totalCommit);
            idoInfo[0].totalCommit += whitelistInfo.hardCap.sub(whitelistInfo.totalCommit);
        }
        emit CommitWhitelist(msg.sender, _amount);
    }


    function claimWhitelistToken() external {
        require(block.timestamp >= claimTime, "Time fail!");
        require(whitelistClaimed[msg.sender] == 0, "User claimed");
        require(whitelistCommitted[msg.sender] > 0, "User not committed");
        IdoInfo memory whitelistInfo = idoInfo[0];
        require(whitelistInfo.claimed, "Invalid status!");
        uint tokenAmount = whitelistCommitted[msg.sender].mul(whitelistInfo.rateToken).div(whitelistInfo.rateEth);
        whitelistClaimed[msg.sender] = tokenAmount;
        require(token.balanceOf(address(this)) >= tokenAmount, "Insufficient token value!");
        token.transfer(msg.sender, tokenAmount);
        //Emit event
        emit ClaimWhitelistToken(msg.sender, tokenAmount);
    }

    function commitFairlaunch() payable external {
        //Check time
        uint _amount = msg.value;
        IdoInfo memory fairlaunchInfo = idoInfo[1];
        require(started, "not started");
        require(block.timestamp >= fairlaunchTime && block.timestamp <= fairlaunchEndTime, "Time invalid!");
        require(fairlaunchInfo.maxCommit >= _amount + fairlaunchCommitted[msg.sender], "Insufficient value!");
        require(_amount + fairlaunchCommitted[msg.sender] >= fairlaunchInfo.minCommit, "value too small!");

        fairlaunchCommitted[msg.sender] += _amount;
        idoInfo[1].totalCommit += _amount;
        emit CommitFairlaunch(msg.sender, _amount);
    }


    function claimFairlaunchToken() external {
        require(block.timestamp >= claimTime, "Time fail!");
        require(fairlaunchClaimed[msg.sender] == 0, "User claimed");
        require(fairlaunchCommitted[msg.sender] > 0, "User not committed");
        IdoInfo memory fairlaunchInfo = idoInfo[1];
        require(fairlaunchInfo.claimed, "Invalid status!");
        uint tokenAmount = fairlaunchInfo.totalCommit < fairlaunchInfo.hardCap ? fairlaunchCommitted[msg.sender].mul(fairlaunchInfo.rateToken).div(fairlaunchInfo.rateEth) :
        fairlaunchCommitted[msg.sender].mul(fairlaunchInfo.rateToken).div(fairlaunchInfo.totalCommit);
        fairlaunchClaimed[msg.sender] = tokenAmount;
        require(token.balanceOf(address(this)) >= tokenAmount, "Insufficient token value!");
        token.transfer(msg.sender, tokenAmount);
        //Emit event
        emit ClaimFairlaunchToken(msg.sender, tokenAmount);
    }

    function refundWhitelist() external {
        require(block.timestamp >= claimTime, "Time fail!");
        require(refund, "Pool can't refund!");
        require(whitelistCommitted[msg.sender] > 0, "User not committed");
        uint256 committed = whitelistCommitted[msg.sender];
        whitelistCommitted[msg.sender]=0;
        (bool sent, ) = payable(msg.sender).call{value: committed}("");
        require(sent, "Failed to send Ether refundWhitelist");
        emit RefundWhitelist(msg.sender, committed);
    }

    function refundFairlaunch() external {
        require(block.timestamp >= claimTime, "Time fail!");
        require(refund, "Pool can't refund!");
        require(fairlaunchCommitted[msg.sender] > 0, "User not committed");
        uint256 committed = fairlaunchCommitted[msg.sender];
        fairlaunchCommitted[msg.sender]=0;
        (bool sent, ) = payable(msg.sender).call{value: committed}("");
        require(sent, "Failed to send Ether refundFairlaunch");
        emit RefundFairlaunch(msg.sender, committed);
    }


    /*  ╔══════════════════════════════╗
        ║        PUBLIC GETTERS        ║
        ╚══════════════════════════════╝ */

    function getTimes() public view returns (uint256 _whitelistTime, uint256 _whitelistEndTime, uint256 _fairlaunchTime, uint256 _fairlaunchEndTime, uint256 _claimTime) {
        return (whitelistTime, whitelistEndTime, fairlaunchTime, fairlaunchEndTime, claimTime);
    }

    function getWhitelistTokenAmount(address _user) public view returns (uint256 _tokenAmount) {
        IdoInfo memory whitelistInfo = idoInfo[0];
        if(whitelistClaimed[_user] > 0 || whitelistCommitted[_user] == 0) return 0;
        return whitelistCommitted[_user].mul(whitelistInfo.rateToken).div(whitelistInfo.rateEth);
    }

    function getFairlaunchTokenAmount(address _user) public view returns (uint256 _tokenAmount) {
        IdoInfo memory fairlaunchInfo = idoInfo[1];
        if(fairlaunchClaimed[_user] > 0 || fairlaunchCommitted[_user] == 0) return 0;

        uint tokenAmount = fairlaunchInfo.totalCommit < fairlaunchInfo.hardCap ? fairlaunchCommitted[_user].mul(fairlaunchInfo.rateToken).div(fairlaunchInfo.rateEth) :
        fairlaunchCommitted[_user].mul(fairlaunchInfo.rateToken).div(fairlaunchInfo.totalCommit);

        return tokenAmount;
    }


    receive() external payable {}

    fallback() external payable {}
}