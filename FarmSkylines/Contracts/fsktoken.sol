contract FSKToken is Context, IERC20, IERC20Metadata, Ownable {
    mapping(address => uint256) private _balances;

    mapping(address => mapping(address => uint256)) private _allowances;

    uint256 private _totalSupply;

    string private _name;

    string private _symbol;

    address public rewordsAddress;
    address public pancakeswapPair;
    address public teamAddress;
    uint256 private _rewordsFee = 8;
    uint256 private _teamFee = 2;
    uint256 private _liquidityFee = 2;
    uint256 private _minTokenFeeToLiquidityAndMarket;
    IPancakeRouter02 public pancakeRouter;
    bool private _feeChecked;
    bool private _canSwapFee=true;
    bool private _canTakeFee=true;
    bool private _canTakeForNormalTxFee;
    mapping(address=>bool) public isNoFeeAddress;
    event SwapAndCharged(uint256 token, uint256 liquidAmount, uint256 bnbPool,  uint256 bnbLiquidity);
    constructor(address _pancakeRouter, address _rewords, address _team,uint256 totalSupply_){
        _name = "FARM SKYLINES TOKEN";
        _symbol = "FSK";
        rewordsAddress = _rewords;
        teamAddress = _team;
        totalSupply_ = totalSupply_*10**18;
        _mint(_msgSender(), totalSupply_);
        isNoFeeAddress[_msgSender()]=true;
        _minTokenFeeToLiquidityAndMarket=totalSupply_*25/100000;  //0.025%
        pancakeRouter =  IPancakeRouter02(_pancakeRouter);
        pancakeswapPair = IPancakeswapV2Factory(pancakeRouter.factory())
            .createPair(address(this), pancakeRouter.WETH());
    }

    receive() external payable {}

    function setPancakeRouter(address addr) external onlyOwner{
        require(addr!=address(0),"zero address not allowed");
        pancakeRouter =  IPancakeRouter02(addr);
    }

    function setRewordsAddress(address addr) external onlyOwner{
        require(addr!=address(0),"zero address not allowed");
        rewordsAddress =  addr;
    }

    function setTeamAddress(address addr) external onlyOwner{
        require(addr!=address(0),"zero address not allowed");
        teamAddress =  addr;
    }

    function setMinTokenFeeToLiquidityAndMarket(uint256 value) external onlyOwner{
        require(value>=0,"zero not allowed");
        _minTokenFeeToLiquidityAndMarket =  value;
    }

    function setRewordsFee(uint256 value) external onlyOwner{
        require(value<20,"invalid");
        _rewordsFee =  value;
    }

    function setTeamFee(uint256 value) external onlyOwner{
        require(value<20,"invalid");
        _teamFee =  value;
    }

    function setLiquidityFee(uint256 value) external onlyOwner{
        require(value<20,"invalid");
        _liquidityFee =  value;
    }

    function setCanSwapFee(bool bl) external onlyOwner{
        _canSwapFee =  bl;
    }

    function setCanTakeFee(bool bl) external onlyOwner{
        _canTakeFee =  bl;
    }

    function setCanTakeForNormalTxFee(bool bl) external onlyOwner{
        _canTakeForNormalTxFee =  bl;
    }

    function setNoFeeAddress(address addr,bool bl) external onlyOwner{
        isNoFeeAddress[addr] =  bl;
    }

    function name() public view virtual override returns (string memory) {
        return _name;
    }

    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    function decimals() public view virtual override returns (uint8) {
        return 18;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view virtual override returns (uint256) {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount) public virtual override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view virtual override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public virtual override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        _transfer(sender, recipient, amount);

        uint256 currentAllowance = _allowances[sender][_msgSender()];
        require(currentAllowance >= amount, "ERC20: transfer amount exceeds allowance");
        unchecked {
        _approve(sender, _msgSender(), currentAllowance - amount);
        }

        return true;
    }


    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender] + addedValue);
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        uint256 currentAllowance = _allowances[_msgSender()][spender];
        require(currentAllowance >= subtractedValue, "ERC20: decreased allowance below zero");
        unchecked {
       _approve(_msgSender(), spender, currentAllowance - subtractedValue);
        }

        return true;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        uint256 feeLiquidity;
        bool isSwap=msg.sender == pancakeswapPair ||
                msg.sender == address(pancakeRouter);
        if(
            recipient != address(this) &&
            sender!=address(this) &&
            _canTakeFee &&
            !isNoFeeAddress[tx.origin]
        ){
            //normal tx or swap tx
            if(
                (!isSwap&&
                _canTakeForNormalTxFee)||
                isSwap
            ){
               feeLiquidity = amount * (_liquidityFee)/100;
            }
        }

        if(!_feeChecked){
              amount = _chargeFee(sender,recipient,amount);
        }
        _beforeTokenTransfer(sender, recipient, amount);

        uint256 senderBalance = _balances[sender];
        require(senderBalance >= amount, "ERC20: transfer amount exceeds balance");
        unchecked {
          _balances[sender] = senderBalance - amount;
        }
        _balances[recipient] += amount;

        emit Transfer(sender, recipient, amount);

        _afterTokenTransfer(sender, recipient, amount);
    }

    function changeFee()external{
         bool canSendFee=balanceOf(address(this))>=_minTokenFeeToLiquidityAndMarket;
         if(canSendFee){
            _swapAndCharge(_minTokenFeeToLiquidityAndMarket);
         }
    }

    function _chargeFee(address sender,address recipient, uint256 amount)private returns(uint256){
        uint256 feeValue;
        uint256 feeLiquidity;
        bool isSwap=msg.sender == pancakeswapPair ||
                msg.sender == address(pancakeRouter);
        if(
            recipient != address(this) &&
            sender!=address(this) &&
            _canTakeFee &&
            !isNoFeeAddress[tx.origin]
        ){
            //normal tx or swap tx
            if(
                (!isSwap&&
                _canTakeForNormalTxFee)||
                isSwap
            ){
               feeValue=amount*(_rewordsFee + _teamFee + _liquidityFee)/100;
               feeLiquidity = amount * (_liquidityFee)/100;
              _transfer(sender, address(this), feeValue-feeLiquidity);
            }
        }

        bool canSendFee=balanceOf(address(this))>=_minTokenFeeToLiquidityAndMarket;
        //only normal transaction

        if(
            _canSwapFee&&
            canSendFee&&
            !isSwap
         ){
           _swapAndCharge(_minTokenFeeToLiquidityAndMarket);
        }

         return amount-feeValue;
    }

    function _swapAndCharge(uint256 tokenBalance) private {
         _feeChecked=true;
        uint256 initialBalance = address(this).balance; // initialBalance = address(this).balance = 150
        uint256 liquidBalance = 0;
        tokenBalance -= liquidBalance; // tokenBalance = 50
        _swapTokensForBnb(tokenBalance);

        uint256 newBalance = address(this).balance-initialBalance; // 50
        uint256 bnbForLiquid = newBalance*liquidBalance/tokenBalance; // 0
        //_addLiquidity(liquidBalance, bnbForLiquid);
        newBalance-=bnbForLiquid; // 50
        uint256 value=newBalance * _rewordsFee/(_rewordsFee + _teamFee); //25
        (bool success, ) = payable(rewordsAddress).call{value: value}("");
        require(success == true, "_swapAndCharge rewordsAddress Transfer failed.");
        value=newBalance-value; // 25
        (success, ) = payable(teamAddress).call{value: value}("");
        require(success == true, "_swapAndCharge teamAddress Transfer failed.");
        _feeChecked=false;
        emit SwapAndCharged(tokenBalance, liquidBalance, address(this).balance, bnbForLiquid);
    }

    function _swapTokensForBnb(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = pancakeRouter.WETH();

        _approve(address(this), address(pancakeRouter), tokenAmount);

        pancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    function _addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        _approve(address(this), address(pancakeRouter), tokenAmount);

        pancakeRouter.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0,
            0,
            owner(),
            block.timestamp
        );
    }

    function _mint(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: mint to the zero address");

        _beforeTokenTransfer(address(0), account, amount);

        _totalSupply += amount;
        _balances[account] += amount;
        emit Transfer(address(0), account, amount);

        _afterTokenTransfer(address(0), account, amount);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}
}